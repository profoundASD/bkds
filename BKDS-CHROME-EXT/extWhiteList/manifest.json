{
    "manifest_version": 3,
    "name": "BKDS Whitelist Link Blocker",
    "version": "1.0",
    "description": "BKDS Simple URL Blocker",
    "permissions": ["activeTab"],
    "web_accessible_resources": [
      {
        "resources": ["images/*"],
        "matches": ["<all_urls>"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
  }